{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "g++",
            "args": [
                "-std=c++11",
                "-Wall",
                "-Wextra",
                "-g3",
                "${workspaceFolder}/Car.cpp",
                "${workspaceFolder}/ParkingLot.cpp",
                "${workspaceFolder}/main.cpp",
                "-o",
                "${workspaceFolder}/output/ParkingLot"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"]
        }
    ]
}
